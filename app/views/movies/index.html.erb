
<%= button_to "Top Rated Movies", "/users/#{@user.id}/movies?q=top%40rated" %>
<%= form_with url: "/users/#{@user.id}/movies?q=top%40rated", method: :get, local: :true do |f| %><br>
    <%= f.label :movie_title, "Title"  %>
    <%= f.text_field :movie_title %>
    <%= f.submit "Search by Title" %>
    <% end %>